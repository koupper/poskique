                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select   d.id_corte_caja as 'IdentificadorCaja', d.fechaInicio as IniciodelDia,c.id_caja as CodigoSubtotal, c.codigo_empleado as CodigoTrabajador, e.nombre + ' ' + e.apellido Nombre , 
'$' + cast(cast (c.efectivoInicial as  decimal(16,2)) as varchar(30)) as FondoInicial,

'$' + cast(cast (sum(a.monto) as  decimal(16,2)) as varchar(30)) CantidadEsperada, 
'$' + cast(cast ((c.efectivoAlCierre ) as  decimal(16,2)) as varchar(30))  CantidadReal, 
 '$' + cast(cast (c.faltante as  decimal(16,2)) as varchar(30)) faltante, 
 '$' + cast(cast (c.sobrante as  decimal(16,2)) as varchar(30)) sobrante, a.metodoPago MetodoPago, case d.estatus when 1 then 'Abierta' else 'Cerrada' end Estatus from 
formaPago a inner join ventas b on a.id_venta = b.id_venta 
inner join
caja c on c.id_caja = b.id_caja
inner join 
corteCaja d on d.id_corte_caja = c.caja_maestra  
inner join
usuario e on e.codigo_empleado = c.codigo_empleado 
where d.fechaInicio between @fechaIni and @fechaFin
group by d.id_corte_caja, d.fechaInicio ,e.nombre,  e.apellido,  c.codigo_empleado 
,c.id_caja, c.efectivoInicial, a.metodoPago, d.estatus,c.efectivoAlCierre, c.faltante, c.sobrante
  order by  d.estatus desc ;

</CommandText>
